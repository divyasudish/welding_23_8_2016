<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.DataLogActivity" >

    <LinearLayout
        android:id = "@+id/relativeLayout1"
        android:layout_width = "fill_parent"
        android:layout_height = "wrap_content"
        android:layout_marginTop="10dp"
        android:background = "@color/colorCell">
        <TextView
            android:id="@+id/acc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="left"
            android:text="Accel"
            android:textColor="#ffffff"
            android:textSize="12sp"
            android:background="@drawable/cell_shape"
            android:textStyle="normal" />
        <TextView
            android:id="@+id/gyro"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="left"
            android:text="Gyro"
            android:textColor="#ffffff"
            android:textSize="12sp"
            android:background="@drawable/cell_shape"
            android:textStyle="normal" />
        <TextView
            android:id="@+id/mag"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="left"
            android:text="Magneto"
            android:background="@drawable/cell_shape"
            android:textColor="#ffffff"
            android:textSize="12sp"
            android:textStyle="normal" />
        <TextView
            android:id="@+id/baro"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="left"
            android:text="Baro"
            android:background="@drawable/cell_shape"
            android:textColor="#ffffff"
            android:textSize="12sp"
            android:textStyle="normal" />
        <TextView
            android:id="@+id/temp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="left"
            android:text="Temp"
            android:background="@drawable/cell_shape"
            android:textColor="#ffffff"
            android:textSize="12sp"
            android:textStyle="normal" />
        <TextView
            android:id="@+id/vel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="left"
            android:text="Velocity"
            android:background="@drawable/cell_shape"
            android:textColor="#ffffff"
            android:textSize="12sp"
            android:textStyle="normal" />
        <TextView
            android:id="@+id/relEncoder"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="left"
            android:text="RelEncoder"
            android:background="@drawable/cell_shape"
            android:textColor="#ffffff"
            android:textSize="12sp"
            android:textStyle="normal" />
        <TextView
            android:id="@+id/absEncoder"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="left"
            android:text="AbsEncoder"
            android:background="@drawable/cell_shape"
            android:textColor="#ffffff"
            android:textSize="12sp"
            android:textStyle="normal" />
    </LinearLayout>
    <ListView
        android:id="@+id/dataLogList"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:layout_below="@+id/relativeLayout1"
        android:divider = "@null">
    </ListView>
</RelativeLayout>